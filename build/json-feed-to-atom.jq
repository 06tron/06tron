[
	"<?xml version=\"1.0\" encoding=\"utf-8\"?>",
	"<feed xmlns=\"http://www.w3.org/2005/Atom\"><title>",
	.title,
	"</title><subtitle>",
	.description,
	"</subtitle><id>",
	._atom_elements.id,
	"</id><updated>",
	._atom_elements.updated,
	"</updated>",
	(.authors[] | [
		"<author><name>",
		.name,
		"</name><uri>",
		.url,
		"</uri></author>"
	]),
	"<link rel=\"self\" href=\"",
	._atom_elements.self_link,
	"\"/><icon>",
	.icon,
	"</icon>",
	(.items[] | [
		"<entry><title>",
		.title,
		"</title><id>",
		.id,
		"</id><published>",
		.date_published,
		"</published><updated>",
		.date_modified,
		"</updated><content type=\"html\">",
		(.content_html | @html),
		"</content><link rel=\"alternate\" href=\"",
		.url,
		"\"/></entry>"
	]),
	"</feed>"
] | flatten | join("")
